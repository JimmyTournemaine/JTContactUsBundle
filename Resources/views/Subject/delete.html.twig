{% extends 'base.html.twig' %}
{% trans_default_domain "JTContactUsBundle" %}
{% block title %}{{ 'subject.delete.title'|trans }}{% endblock %}
{% block body %}
<h1>{{ 'subject.delete.title'|trans }}</h1>
{{ form_start(form) }}
	<p>{{ 'subject.delete.confirm'|trans({'%label%': subject.label}) }}</p>
	<a href="{{ path('jt_contact_us_subject_index') }}">{{ 'subject.back'|trans }}</a>
	<input type="submit" value="{{ 'subject.delete.submit'|trans }}" />
{{ form_end(form) }}
{% endblock body %}
